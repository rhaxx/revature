package com.revature.collections;

import java.util.HashSet;
import java.util.Set;

public class UnderstandingCollections {

	public static void main(String[] args) {
		
		Set<String> mySet = new HashSet<String>();
		mySet.add("Hello"); // 1
		mySet.add("my");    // 2
		mySet.add("name");  // 3
		mySet.add("ERROR"); // 4 - removing this later
		mySet.add("is");    // 5
		mySet.add("is");    // 6 - is a duplicate, doesn't get added
		System.out.println(mySet.add("is")); // 7 - adding another is, returns false
		mySet.add("Rex");   // 8
		System.out.println(mySet.size()); // prints out 5
		mySet.remove("ERROR");
		System.out.println(mySet.size()); // prints out 5
		
		
	}
}
